{
    "name": "infoalto/admin",
    "description": "Este Ã© um pacote de admin criado para a infoalto",
    "authors": [
        {
            "name": "Gabriel Santos",
            "email": "gabmaxsan@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.*",
        "laravelcollective/html": "^5.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infoalto\\Admin\\MainServiceProvider"
            ]
        }
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Infoalto\\Admin\\": "src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php artisan storage:link",
            "@php artisan vendor:publish --tag=admin --force",
            "@composer dump-autoload"
        ]
    }
}
